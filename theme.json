{
    "$schema": "http://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
        "appearanceTools": false,
		"custom": {
			"layout": {
				"contentSize": "45rem",
				"narrowSize": "34em",
				"mediumSize": "61.875em",
				"wideSize": "1180px"
			},
			"typography": {
				"font": "\"Montserrat\", sans-serif",
				"scale": 1.125,
				"normal": "1rem",
				"tiny": "calc(var(--wp--custom--typography--small) / var(--wp--custom--typography--scale))",
				"small": "calc(var(--wp--custom--typography--normal) / var(--wp--custom--typography--scale))",
				"normalx": "calc(var(--wp--custom--typography--normal) * var(--wp--custom--typography--scale))",
				"medium": "calc(var(--wp--custom--typography--normalx) * var(--wp--custom--typography--scale))",
				"mediumx": "calc(var(--wp--custom--typography--medium) * var(--wp--custom--typography--scale))",
				"large": "calc(var(--wp--custom--typography--mediumx) * var(--wp--custom--typography--scale))",
				"largex": "calc(var(--wp--custom--typography--large) * var(--wp--custom--typography--scale))",
				"huge": "calc(var(--wp--custom--typography--largex) * var(--wp--custom--typography--scale))",
				"hugex": "calc(var(--wp--custom--typography--huge) * var(--wp--custom--typography--scale))",
				"massive": "calc(var(--wp--custom--typography--hugex) * var(--wp--custom--typography--scale))",
				"massivex": "calc(var(--wp--custom--typography--massive) * var(--wp--custom--typography--scale))"
			},
			"wsuColorCore": {
				"crimson": "#a60f2d",
				"crimsonLight": "#ca1237",
				"gray": "#4d4d4d"
			},
			"wsuColorBlack": {
				"100": "hsl(0, 0%, 0%)",
				"90": "hsl(0, 0%, 10%)",
				"80": "hsl(0, 0%, 20%)",
				"70": "hsl(0, 0%, 30%)",
				"60": "hsl(0, 0%, 40%)",
				"50": "hsl(0, 0%, 50%)",
				"40": "hsl(0, 0%, 60%)",
				"30": "hsl(0, 0%, 70%)",
				"20": "hsl(0, 0%, 80%)",
				"10": "hsl(0, 0%, 90%)",
				"5": "hsl(0, 0%, 95%)",
				"3": "hsl(0, 0%, 98%)",
				"0": "hsl(0, 0%, 100%)"
			},
			"wsuColorAccent": {
				"crimson": "#fae6ea",
				"autumn": "#ff6727",
				"goldfinch": "#f3e700",
				"vineyard": "#aadc24",
				"pacificSky": "#5bc3f5",
				"midnight": "#002d61"
			}
		},
        "border": {},
		"color": {
            "background": true,
			"custom": false,
			"customDuotone": false,
			"customGradient": false,
            "defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"link": false,
            "text": true,
			"palette": [
				{
					"name": "Crimson",
					"slug": "primary-crimson",
					"color": "var(--wp--custom--wsu-color-core--crimson)"
				},
				{
					"name": "Gray",
					"slug": "primary-gray",
					"color": "var(--wp--custom--wsu-color-core--gray)"
				},
				{
					"name": "Secondary Crimson",
					"slug": "secondary-crimson",
					"color": "var(--wp--custom--wsu-color-core--crimson-light)"
				},
				{
					"name": "Light",
					"slug": "light",
					"color": "var(--wp--custom--wsu-color-black--3)"
				},
				{
					"name": "Dark",
					"slug": "dark",
					"color": "var(--wp--custom--wsu-color-black--90)"
				}
			]
		},
		"layout": {
			"contentSize": "var(--wp--custom--layout--content-size)",
			"wideSize": "var(--wp--custom--layout--wide-size)"
		},
		"spacing": {
			"margin": false,
			"padding": false,
			"units": [ "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
            "fontFamilies": [],
            "fontSizes": [
				{
					"name": "Small",
					"size": "var(--wp--custom--typography--small)",
					"slug": "small"
				},
				{
					"name": "Normal",
					"size": "var(--wp--custom--typography--normal)",
					"slug": "normal"
				},
				{
					"name": "Medium",
					"size": "var(--wp--custom--typography--medium)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "var(--wp--custom--typography--large)",
					"slug": "large"
				},
				{
					"name": "Huge",
					"size": "var(--wp--custom--typography--huge)",
					"slug": "huge"
				}
			],
            "fontStyle": true,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
            "textDecoration": false,
            "textTransform": true
		},
		"blocks": {
			"core/button": {
				"color": {
					"palette": []
				},
				"typography": {
					"fontSizes": []
				}
			},
			"core/heading": {
				"color": {
					"palette": []
				},
				"typography": {
					"fontSizes": []
				}
			},
			"core/pullquote": {
				"border": {
					"color": true,
					"style": false,
					"radius": false,
					"width": false
				},
				"typography": {
					"fontSizes": [],
					"fontStyle": false
				}
			},
			"core/quote": {
				"typography": {
					"fontSizes": [],
					"fontStyle": false,
					"textTransform": false
				}
			},
			"core/search": {
				"color": {
					"palette": []
				}
			},
			"core/separator": {
				"color": {
					"palette": []
				}
			},
			"core/social-links": {
				"color": {
					"palette": []
				}
			},
			"core/table": {
				"color": {
					"palette": []
				},
				"typography": {
					"fontSizes": [],
					"fontStyle": false,
					"textTransform": false
				}
			}
		}
	}
}
