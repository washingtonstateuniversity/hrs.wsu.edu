{
  "name": "hrs.wsu.edu",
  "version": "0.20.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/washingtonstateuniversity/hrs.wsu.edu"
  },
  "author": "washingtonstateuniversity",
  "contributors": [
    {
      "name": "Adam Turner"
    }
  ],
  "license": "GPL-2.0+",
  "browserslist": [
    "> 1%",
    "ie 11",
    "Firefox ESR"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "autoprefixer": "^9.3.x",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.x",
    "cssnano": "^4.1.7",
    "eslint": "^5.8.0",
    "eslint-config-wordpress": "^2.0.x",
    "eslint-loader": "^2.1.1",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.0.0",
    "rimraf": "^2.6.x",
    "stylelint": "^9.7.1",
    "svgo": "^1.1.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4"
  },
  "scripts": {
    "test:php": "vendor/bin/phpcs -v --colors --extensions=php --ignore=\"*/vendor/*,*/node_modules/*\" --standard=phpcs.ruleset.xml *.php parts/ includes/ articles/ templates/",
    "test:js": "eslint src/js/**",
    "test:css": "stylelint \"src/scss/**/*.scss\"",
    "dev:script": "webpack --progress",
    "style:sass": "node-sass --output-style expanded src/scss -o assets/css",
    "style:prefix": "postcss assets/css/style.css -u autoprefixer --no-map -o ./style.css",
    "style:nano": "postcss assets/css/*.css --map -u autoprefixer -u cssnano --replace",
    "script:webpack": "webpack --env.production --progress",
    "image:setup": "mkdirp assets/images/icons",
    "image:icon": "svgo --config=.svgo.yml -f src/images/svg -o assets/images/icons",
    "image:move": "cpx \"src/images/*.{png,jpg,gif}\" assets/images/ --update",
    "build:style": "run-s \"style:*\"",
    "build:script": "run-p \"script:*\"",
    "build:image": "run-s \"image:*\"",
    "clean": "rimraf assets/{css/,images/,js/}",
    "test": "run-p \"test:*\"",
    "prebuild": "run-p clean test",
    "build": "run-p \"build:*\""
  },
  "dependencies": {
    "url-search-params-polyfill": "^5.0.0"
  }
}
